# Gruvbox Dark (med borders)

# Base colors
set $cl_high   #fabd2f
set $cl_focus  #3c3836
set $cl_back   #282828
set $cl_fore   #ebdbb2
set $cl_indi   #fe8019
set $cl_border #504945
set $cl_urge   #fb4934

# Window colors
client.focused          $cl_high $cl_focus $cl_fore $cl_indi $cl_high
client.focused_inactive $cl_border $cl_back $cl_fore $cl_border $cl_border
client.unfocused        $cl_border $cl_back $cl_fore $cl_border $cl_border
client.urgent           $cl_urge $cl_urge $cl_fore $cl_urge $cl_urge

